{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "core.settings",
      "PYTHONUNBUFFERED": "1"
    }
  }
  