{
    "builds": [
        {
            "src": "core/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.9" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "demo/wsgi.py"
        }
    ]
}